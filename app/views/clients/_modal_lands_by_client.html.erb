<div class="modal-dialog modal-lg" role="document">
  <div class="modal-content">
    <div class="modal-header">
      <h5 class="modal-title"><%= @title_modal%></h5>
      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
    <div class="modal-body">
      <table id="clients_table" class="table hover dt-responsive w-100">
        <thead>
          <tr>
            <th>Manzana</th>
            <th>Lote</th>
            <th>Acciones</th>
          </tr>
        </thead>
        <tbody>
          <% @lands.each do |land|%>
            <tr>
              <td> <%= land.apple.code %> </td>
              <td> <%= land.code %> </td>
              <td> <%= link_to '<i class="icon-real-estate-044 u-line-icon-pro"></i>'.html_safe, land_detail_sales_path( id: land.id ) ,
                        class: 'u-link-v5 g-color-gray g-color-secondary--hover g-text-underline--none--hover g-ml-12', title: 'Ir a detalle de venta'  %> </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>